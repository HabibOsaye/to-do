{
	"name": "to-do",
	"version": "1.0.0",
	"scripts": {
		"start": "npm run dump & parcel serve index.html -p 5024",
		"dump": "rm -rf .cache .parcel-cache dist & clear"
	},
	"author": "Habib T Osaye",
	"license": "ISC",
	"devDependencies": {
		"@parcel/transformer-image": "^2.0.0-rc.0",
		"@parcel/transformer-sass": "^2.0.0-rc.0",
		"autoprefixer": "^10.3.1",
		"cssnano-preset-default": "^5.1.3",
		"parcel": "^2.0.0-rc.0",
		"postcss": "^8.3.6",
		"sass": "^1.37.5"
	},
	"dependencies": {
		"date-fns": "^2.23.0",
		"uuid": "^8.3.2"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {
				"overrideBrowserslist": [
					"last 2 version",
					"ie >= 11",
					"Android >= 4.4"
				]
			},
			"cssnano": {
				"preset": [
					"default",
					{
						"discardComments": {
							"removeAll": true
						}
					}
				]
			},
			"cssnano-preset-default": {}
		}
	}
}
